<default:ReloadedWindow x:Class="Reloaded.Mod.Launcher.Pages.Dialogs.InstallModPackDialog"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                xmlns:local="clr-namespace:Reloaded.Mod.Launcher.Pages.Dialogs"
                xmlns:default="clr-namespace:Reloaded.WPF.Theme.Default;assembly=Reloaded.WPF.Theme.Default"
                xmlns:controls="clr-namespace:Reloaded.WPF.Controls;assembly=Reloaded.WPF"
                xmlns:converters="clr-namespace:Reloaded.Mod.Launcher.Converters"
                mc:Ignorable="d"
                Title="{DynamicResource InstallModPackTitle}" 
                Width="1280"
                Height="720"
                WindowStartupLocation="CenterScreen"
                Style="{DynamicResource ReloadedWindow}">

    <!-- Child Page -->
    <controls:PageHost x:Name="PageHost" 
                       ClipToBounds="True"
                       Width="Auto"
                       Height="Auto"
                       VerticalAlignment="Stretch"
                       HorizontalAlignment="Stretch" />

</default:ReloadedWindow>
